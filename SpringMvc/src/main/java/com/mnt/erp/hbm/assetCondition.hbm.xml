<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.mnt.erp.bean.AssetConditionBean" table="AssetCondition">
		<id name="assetConditionId" column="AssetCondition_Id">
			<generator class="increment"></generator>
		</id>
		<property name="assetConditionCode" column="AssetConditionCode"></property>
		<property name="assetCondition" column="AssetCondition"></property>
	</class>
</hibernate-mapping>