<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.mnt.erp.bean.CustomerBankDept" table="CustomerBankDet">
		<id name="customerBankDetId" column="CustomerBankDet_id" type="int">
			<generator class="increment"></generator>
		</id>
		<property name="bankName" column="BankName"></property>
		<property name="bankAddress" column="BankAddress"></property>
		<property name="accountType" column="AccountType"></property>
		<property name="accountNumber" column="AccountNumber"></property>
		<property name="MICRCode" column="MICRCode"></property>
		<property name="IFSCCode" column="IFSCCode"></property>
	 	
	 	
	</class>
</hibernate-mapping>