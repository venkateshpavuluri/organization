<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.mnt.erp.bean.ReceiptWithHold" table="ReceiptWithHold">
		<id name="recWithHoldId" column="ReceiptWithHold_Id" type="int">
			<generator class="increment"></generator>
		</id>
		<property name="withHoldReason" column="WithHoldReason"></property>
		<property name="withHoldAmount" column="WithHoldAmount"></property>

	</class>
</hibernate-mapping>