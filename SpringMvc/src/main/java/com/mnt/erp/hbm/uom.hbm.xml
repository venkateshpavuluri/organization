<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.mnt.erp.bean.Uom" table="Uom">

<id name="uom_Id" column="UOM_Id">
<generator class="increment"></generator>
</id>

<property name="uom" column="UOM" unique="true"></property>
<property name="uomCode" column="UOMCode" unique="true"></property>

</class>
</hibernate-mapping>
