<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.mnt.erp.bean.VendorDocuments" table="VendorDocuments">
<id name="vendorDocId" column="VendorDoc_Id">
<generator class="increment"></generator>
</id>
<property name="documentName" column="documentName"></property>
<property name="documentPath" column="documentPath"></property>
<property name="vendorId" column="Vendor_Id"></property>
</class>

</hibernate-mapping>
