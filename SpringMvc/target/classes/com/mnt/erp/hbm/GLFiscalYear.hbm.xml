<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.mnt.erp.bean.GLFiscalYear" table="GLFiscalYear" >
		<id name="gLFiscalYear_Id" column="GLFiscalYear_Id" type="int">
			<generator class="increment"></generator>
		</id>
		<property name="fiscalYear" column="FiscalYear"></property>
		<property name="calendarYear" column="CalendarYear"></property>
		<property name="startDate" column="StartDate"></property>
		<property name="endDate" column="EndDate"></property>
		<property name="fiscalYearClosed" column="FiscalYearClosed"></property>

	</class>

</hibernate-mapping>