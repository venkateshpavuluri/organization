<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.mnt.erp.bean.MaintenanceCategory" table="MaintenanceCategory">

		<id name="maintenanceCategoryId" column="MaintenanceCategory_Id">
			<generator class="increment"></generator>
		</id>
		<property name="maintenanceCategory" column="MaintenanceCategory"></property>
	</class>
</hibernate-mapping>