<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.mnt.erp.bean.Shop" table="Shop">

		<id name="shop_Id" column="Shop_Id">
			<generator class="increment"></generator>
		</id>

		<property name="shopCode" column="ShopCode"></property>
		<property name="shopName" column="ShopName"></property>
		<property name="description" column="Description"></property>
		<property name="plant_Id" column="Plant_Id"></property>
		

        
	</class>
</hibernate-mapping>