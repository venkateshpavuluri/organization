<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.mnt.erp.bean.VendGroup" table="VendGroup">

<id name="vendorGroup_Id" column="VendorGroup_Id">
<generator class="increment"></generator>
</id>

<property name="vendorGroup" column="VendorGroup" unique="true"></property>
<property name="vendorGroupCode" column="VendorGroupCode" unique="true"></property>

</class>
</hibernate-mapping>
