<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.mnt.erp.bean.Currency" table="Currency">

<id name="currencyId" column="Currency_Id">
<generator class="increment"></generator>
</id>
<property name="currency" column="Currency"></property>
<property name="isoCode" column="ISOcode"></property>
<property name="symbol" column="Symbol"></property>
</class>
</hibernate-mapping>
