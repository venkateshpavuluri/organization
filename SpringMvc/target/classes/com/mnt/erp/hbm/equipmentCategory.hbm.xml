<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.mnt.erp.bean.EquipmentCategory" table="EquipmentCategory">
		<id name="equipmentCategoryId" column="EquipmentCategory_Id">
			<generator class="increment"></generator>
		</id>
		<property name="equipmentCategory" column="EquipmentCategory"></property>
	</class>
</hibernate-mapping>
