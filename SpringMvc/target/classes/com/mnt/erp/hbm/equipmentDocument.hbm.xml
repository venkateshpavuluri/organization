<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.mnt.erp.bean.EquipmentDocument" table="EquipmentDocuments" >
		<id name="equipmentDocumentId" column="EquipmentDocument_Id" type="int">
			<generator class="increment"></generator>
		</id>
		<property name="equipmentId" column="Equipment_Id"></property>
		<property name="documentName" column="DocumentName"></property>
		<property name="documentPath" column="DocumentPath"></property>
	</class>
</hibernate-mapping>