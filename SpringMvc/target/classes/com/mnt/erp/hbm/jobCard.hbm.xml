<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.mnt.erp.bean.JobCardBean" table="Jobcard" >
		<id name="jobCardId" column="Jobcard_Id" type="int">
			<generator class="increment"></generator>
		</id>
		<property name="prodBatchId" column="ProdBatch_Id"></property>
		<property name="jobcardDate" column="JobcardDT"></property>

	</class>

</hibernate-mapping>