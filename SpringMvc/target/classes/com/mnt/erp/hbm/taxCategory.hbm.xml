<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.mnt.erp.bean.TaxCategory" table="TaxCategory">
<id name="taxCategoryId" column="TaxCategory_Id">
<generator class="increment"></generator>
</id>
<property name="taxCategory" column="TaxCategory"></property>
<property name="taxCategoryCode" column="TaxCategoryCode"></property>
</class>
</hibernate-mapping>
