<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.mnt.erp.bean.TrainingCategory" table="TrainingCategory">
		<id name="trainingCategoryId" column="TrainingCat_Id">
			<generator class="increment"></generator>
		</id>
		<property name="trainingCategory" column="TrainingCategory"></property>
	</class>
</hibernate-mapping>